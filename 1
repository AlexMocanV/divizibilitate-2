#include <iostream>

using namespace std;



int main()
{
    int n,i,nr=0,s=0;
    cin>>n;
    for(i=2;i<n;i++)
    {
        if(n%i==0&&nr<2)
        {
            s+=i;
            nr++;
        }
        if(nr==2)
        {
            break;
        }
        
    }
    cout<<s;

    return 0;
}
